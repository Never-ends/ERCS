@model ERCS.ViewModels.VirusVMs.VirusVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.VirusName" />
<wt:display field="Entity.VirusCode" />
<wt:display field="Entity.VirusRemark" />
<wt:display field="Entity.VirusType" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>